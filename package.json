{
  "name": "vscode-translate",
  "displayName": "vscode translate",
  "description": "翻译小插件 translate tool",
  "version": "0.1.0",
  "author": "Gabe Yuan[yugang2002@gmail.com]",
  "publisher": "fishjar",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishjar/vscode-translate.git"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-translate.translate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-translate.translate",
        "title": "Mihuan:translate"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-translate.translate",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.38.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
